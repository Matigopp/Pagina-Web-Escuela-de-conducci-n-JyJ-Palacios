{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.js" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/usuarios/?", "dest": "/public/Usuarios.html" },
    { "src": "/contenidos/?", "dest": "/public/ListaDeContenidos.html" },
    { "src": "/cursos/?", "dest": "/public/CursosDisponibles.html" },
    { "src": "/contacto/?", "dest": "/public/Contacto.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
